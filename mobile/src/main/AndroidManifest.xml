<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.luiseduardo.infrafacil">
    <!-- package="com.example.luiseduardo.autocompletetextview"> -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.CAMERA" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/network"
        android:label="Proatec Control"
        android:roundIcon="@mipmap/network"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        android:windowSoftInputMode="adjustPan">

        <activity android:name="com.example.luiseduardo.infrafacil.Produtos"></activity>
        <activity
            android:name="com.example.luiseduardo.infrafacil.Poker"
            android:label="Poker - Jogo."
            android:windowSoftInputMode="stateHidden"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CalendarActivity"
            android:label="Calendário dos Jogos" />

        <activity
            android:name="com.example.luiseduardo.infrafacil.Poker_main"
            android:label="Poker - Jogo."
            android:windowSoftInputMode="stateHidden"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name = "android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.luiseduardo.infrafacil.Poker_new"
            android:label="Poker - Jogo."
            android:windowSoftInputMode="stateHidden"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.luiseduardo.infrafacil.Financas"
            android:label="@string/title_activity_financas"
            android:theme="@style/AppTheme.NoActionBar" />

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/circular_progress_bar" />

        <activity
            android:name="com.example.luiseduardo.infrafacil.MainActivity"
            android:label="Proatec Control - DashBoardd"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.luiseduardo.infrafacil.DadosLogin"
            android:label="Proatec Control - Login"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="true"
            tools:ignore="Instantiatable">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.luiseduardo.infrafacil.Login"
            android:label="Proatec Control - Login"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.luiseduardo.infrafacil.Ordem"
            android:label="Proatec Control"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.luiseduardo.infrafacil.Cadastro_Clientes"
            android:label="@string/app_name"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.luiseduardo.infrafacil.Status_Ordem"
            android:label="Proatec - Descrição Ocor."
            android:windowSoftInputMode="adjustResize|adjustPan"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.luiseduardo.infrafacil.Clientes"
            android:label="Proatec - Clientes."
            android:windowSoftInputMode="stateHidden"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.luiseduardo.infrafacil.NewOrdem"
            android:label="@string/app_name" />


        <meta-data
            android:name="com.google.android.gms.car.application"
            android:resource="@xml/automotive_app_desc" />
        <activity
            android:name="com.example.luiseduardo.infrafacil.Cadastro_Produtos"
            android:label="Adicionar Protutos" />

        <meta-data
            android:name="com.google.android.gms.car.application"
            android:resource="@xml/automotive_app_desc" />
    </application>


</manifest>